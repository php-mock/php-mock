{
    "name": "malkusch/php-mock",
    "type": "library",
    "description": "Mock non deterministic built-in PHP functions (e.g. time() or rand())",
    "keywords": ["mock", "stub", "function", "test"],
    "homepage": "https://github.com/malkusch/php-mock",
    "license": "WTFPL-2.0",
    "authors": [
        {
            "name": "Markus Malkusch",
            "email": "markus@malkusch.de",
            "homepage": "http://markus.malkusch.de",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {"malkusch\\phpmock\\": "classes/"}
    },
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4"
    },
    "archive": {
        "exclude": ["/tests"]
    }
}
